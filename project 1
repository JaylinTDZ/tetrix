#include <PRIZM.h>
PRIZM prizm;

void setup() {
  prizm.PrizmBegin();
  //Pg165
  prizm.setMotorInvert(2,1);
}
void loop() {
  //Drive forward 1m Pg 164
  prizm.setMotorSpeeds(360,360);
  delay(4000);
  //Stop and turn right
  prizm.setMotorSpeeds(-360,360);
  delay(510);
  //Go forward Slow and Stop
    prizm.setMotorSpeeds(360,360);
  delay(1500);
   prizm.setMotorSpeeds(360,-360);
  delay(500);
  //Go forward Slow
     prizm.setMotorSpeeds(360,360);
  delay(900);
  //Go forward 1m Pg 164
    prizm.setMotorSpeeds(360,360);
  delay(2000);
  prizm.setServoPosition(1,180);
  prizm.PrizmEnd();
}
